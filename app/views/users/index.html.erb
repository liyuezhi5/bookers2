<%= render 'books/list', books: @books %>
    
<div class="offset-md-1"></div>
<div class="col-md-8">
<table class="table table-striped">
  <thead>
    <tr>
      <th>image</th>
      <th>name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <h2>Users</h2>
    <% @users.each do |user| %>
      <tr>
        <td><%= image_tag user.get_profile_image(100,100) %></td>
        <td><%= user.name %></td>
        <td><%= link_to "Show", books_path %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
</div>
